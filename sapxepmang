#include <iostream>
#include <math.h>

using namespace std;

void sapxepchen(int a[], int n){ //LV3
    for(int i = 1; i < n; i++){
        int x = a[i], vitri = i - 1;
        while(vitri >= 0 && x < a[vitri]){
            a[vitri + 1] = a[vitri];
            -- vitri;
        }
        a[vitri + 1] = x;
    }
}

void sapxepnoibot(int a[], int n){ //LV2
    for(int i = 0; i < n - 1; i++){
        for(int j = 0; j < n - i - 1; j++){
            if(a[j] > a[j + 1])swap(a[j], a[j + 1]);
        }
    }
}

void sapxepchon(int a[], int n){ // LV1
    for(int i = 0; i < n - 1; i++){
        int min = i;
        for(int j = i + 1; j < n; j++){
            if(a[j] < a[min]){
                min = j;
            }
        }
        swap(a[i], a[min]);
    }
}

int main(){
    int n;
    cin >> n;
    int a[1000];
    for(int i = 0; i < n; i++){
        cin >> a[i];
    }
    sapxepnoibot(a, n);
    for(int i = 0; i < n; i++){
        cout << a[i];
    }
}
